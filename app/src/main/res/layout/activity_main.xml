<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/silueta_fondo"
    android:backgroundTint="#DFEDD6"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/Boton"
        android:layout_width="120pt"
        android:layout_height="20pt"
        android:onClick="crear"
        android:text="@string/insertar_archivo_excel"
        android:backgroundTint="#298531"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="120pt"
        android:layout_height="20pt"
        android:layout_marginStart="122dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="123dp"
        android:backgroundTint="#298531"
        android:text="@string/SIN_DATOS"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Boton"
        tools:visibility="visible"/>

</androidx.constraintlayout.widget.ConstraintLayout>