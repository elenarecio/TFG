<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/silueta_fondo"
    android:backgroundTint="#DFEDD6"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/Boton"
        android:layout_width="120pt"
        android:layout_height="20pt"
        android:backgroundTint="#A3298531"
        android:onClick="crear"
        android:text="@string/insertar_archivo_excel"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Boton2"
        android:layout_width="120pt"
        android:layout_height="20pt"
        android:backgroundTint="#298531"
        android:onClick="botones"
        android:text="@string/SIN_DATOS"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.632" />


</androidx.constraintlayout.widget.ConstraintLayout>